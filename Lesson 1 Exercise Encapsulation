#include <stdio.h>

void squareMultTable(int n, int high) {
    for (int i = 1; i <= high; i++) {
        printf("%d\t", n * i);
    }
    printf("\n");
}

void print_tables(int high) {
    for (int i = 1; i <= high; i++) {
        squareMultTable(i, high);
    }
}

int main(void) {
    print_tables(9);
    return 0;
}
