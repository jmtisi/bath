#include <stdio.h>
#include <string.h>

void printCharsInString(const char *string) {
    int len = strlen(string);
    for (int i = 0; i < len; i++) {
        printf("%c\n", string[i]);
    }
}

int main(void) {
    const char my_string[] = "This is a string";
    printCharsInString(my_string);
    return 0;
}
